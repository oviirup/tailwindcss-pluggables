{
  "name": "tailwindcss-pluggables",
  "version": "1.2.0",
  "description": "A collection of useful tailwind plugins in one package",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/tailwindcss-pluggable"
  },
  "author": {
    "name": "Avirup Ghosh",
    "url": "https://github.com/oviirup"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "style": "./index.css",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./animate": {
      "types": "./dist/animate.d.ts",
      "style": "./animate.css",
      "require": "./dist/animate.js",
      "import": "./dist/animate.mjs"
    },
    "./drag": {
      "types": "./dist/drag.d.ts",
      "style": "./drag.css",
      "require": "./dist/drag.js",
      "import": "./dist/drag.mjs"
    },
    "./hocus": {
      "types": "./dist/hocus.d.ts",
      "style": "./hocus.css",
      "require": "./dist/hocus.js",
      "import": "./dist/hocus.mjs"
    },
    "./inset-center": {
      "types": "./dist/inset-center.d.ts",
      "style": "./inset-center.css",
      "require": "./dist/inset-center.js",
      "import": "./dist/inset-center.mjs"
    }
  },
  "files": [
    "animate.css",
    "dist",
    "drag.css",
    "hocus.css",
    "index.css",
    "inset-center.css"
  ],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "plugin",
    "collection"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier . -wu --cache"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || insiders"
  },
  "devDependencies": {
    "@changesets/changelog-github": "~0.5.1",
    "@changesets/cli": "~2.29.0",
    "@types/node": "~22.14.1",
    "prettier": "~3.5.3",
    "prettier-plugin-jsdoc": "~1.3.2",
    "prettier-plugin-pkg": "~0.19.0",
    "tailwindcss": "~3.4.17",
    "tsup": "~8.5.0",
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
