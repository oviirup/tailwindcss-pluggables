{
  "name": "tailwindcss-pluggables",
  "version": "1.0.2",
  "type": "module",
  "description": "A collection of useful tailwind plugins in one package",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/tailwindcss-pluggable"
  },
  "author": {
    "name": "Avirup Ghosh",
    "url": "https://github.com/oviirup"
  },
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/utils.js",
    "./plugins": "./dist/plugins/index.js",
    "./plugins/*": "./dist/plugins/*.js"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "plugin",
    "collection"
  ],
  "scripts": {
    "build": "tsup --minify",
    "format": "prettier . -w -u --cache",
    "generate": "turbo gen",
    "watch": "tsup --watch"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || insiders"
  },
  "devDependencies": {
    "@changesets/changelog-github": "~0.5.0",
    "@changesets/cli": "~2.27.9",
    "@turbo/gen": "~2.3.0",
    "@types/micromatch": "~4.0.9",
    "@types/node": "~20.17.6",
    "micromatch": "~4.0.8",
    "prettier": "~3.3.3",
    "prettier-plugin-jsdoc": "~1.3.0",
    "prettier-plugin-pkg": "~0.18.1",
    "tailwindcss": "~3.4.14",
    "tsup": "~8.3.5",
    "turbo": "~2.3.0",
    "typescript": "~5.6.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
