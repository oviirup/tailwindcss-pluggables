{
  "name": "tailwindcss-pluggables",
  "version": "1.0.0",
  "type": "module",
  "description": "A collection of useful tailwind plugins in one package",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/tailwindcss-pluggable"
  },
  "author": {
    "name": "Avirup Ghosh",
    "url": "https://github.com/oviirup"
  },
  "license": "MIT",
  "packageManager": "bun@1.1.34",
  "exports": {
    "./plugins/*": "./dist/plugins/*.js"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "plugin",
    "collection"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup -w"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || insiders"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.9.0",
    "micromatch": "~4.0.8",
    "prettier": "~3.3.3",
    "prettier-plugin-jsdoc": "~1.3.0",
    "prettier-plugin-pkg": "~0.18.1",
    "tailwindcss": "~3.4.14",
    "tsup": "~8.3.5",
    "typescript": "~5.6.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
