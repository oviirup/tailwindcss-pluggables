{
  "name": "tailwindcss-pluggables",
  "version": "1.1.1",
  "description": "A collection of useful tailwind plugins in one package",
  "repository": {
    "type": "git",
    "url": "https://github.com/oviirup/tailwindcss-pluggable"
  },
  "author": {
    "name": "Avirup Ghosh",
    "url": "https://github.com/oviirup"
  },
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./animate": "./dist/animate.css",
    "./inset-center": "./dist/inset-center.css",
    "./drag": "./dist/drag.css",
    "./plugins": "./dist/plugins/index.js",
    "./plugins/*": "./dist/plugins/*.js"
  },
  "files": [
    "animate.css",
    "dist",
    "drag.css",
    "inset-center.css"
  ],
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "plugin",
    "collection"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier . -wu --cache",
    "generate": "turbo gen"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0 || insiders"
  },
  "devDependencies": {
    "@changesets/changelog-github": "~0.5.1",
    "@changesets/cli": "~2.29.0",
    "@turbo/gen": "~2.5.0",
    "@types/node": "~22.14.1",
    "prettier": "~3.5.3",
    "prettier-plugin-jsdoc": "~1.3.2",
    "prettier-plugin-pkg": "~0.19.0",
    "tailwindcss": "~3.4.17",
    "tinyglobby": "~0.2.13",
    "tsup": "~8.4.0",
    "turbo": "~2.5.0",
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
